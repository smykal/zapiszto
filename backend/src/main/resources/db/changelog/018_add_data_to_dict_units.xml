<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd">

    <changeSet id="insertData-dict_units" author="smykalm">
        <preConditions onFail="MARK_RAN">
            <tableExists tableName="dict_units" schemaName="public"/>
        </preConditions>

        <insert tableName="dict_units">
            <column name="name" value="kilogram"/>
            <column name="shortcut" value="kg"/>
            <column name="description" value="Description for Thigh"/>
        </insert>

        <insert tableName="dict_units">
            <column name="name" value="minuta"/>
            <column name="shortcut" value="min"/>
            <column name="description" value="Description for Thigh"/>
        </insert>

        <insert tableName="dict_units">
            <column name="name" value="seunda"/>
            <column name="shortcut" value="sec"/>
            <column name="description" value="Description for Thigh"/>
        </insert>


    </changeSet>

</databaseChangeLog>
