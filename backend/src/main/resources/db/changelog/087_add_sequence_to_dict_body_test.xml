<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="add-sequence-dict_body_test_basic" author="smykalm">
        <createSequence sequenceName="seq_dict_body_test_basic_id" startValue="1"/>
    </changeSet>

    <changeSet id="add-sequence-dict_body_test_per_user" author="smykalm">
        <createSequence sequenceName="seq_dict_body_test_per_user_id" startValue="1"/>
    </changeSet>

    <changeSet id="add-sequence-dict_body_test" author="smykalm">
        <createSequence sequenceName="seq_dict_body_test_id" startValue="1"/>
    </changeSet>

    <changeSet id="modify-dict_body_test_basic-id" author="smykalm">
        <addAutoIncrement
                columnName="id"
                tableName="dict_body_test_basic"
                startWith="1"
                incrementBy="1"
                sequenceName="seq_dict_body_test_basic_id"/>
    </changeSet>

    <changeSet id="modify-dict_body_test_per_user-id" author="smykalm">
        <addAutoIncrement
                columnName="id"
                tableName="dict_body_test_per_user"
                startWith="1"
                incrementBy="1"
                sequenceName="seq_dict_body_test_per_user_id"/>
    </changeSet>

    <changeSet id="modify-dict_body_test-id" author="smykalm">
        <addAutoIncrement
                columnName="id"
                tableName="dict_body_test"
                startWith="1"
                incrementBy="1"
                sequenceName="seq_dict_body_test_id"/>
    </changeSet>

</databaseChangeLog>
